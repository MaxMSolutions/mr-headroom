{
  "routes": [
    {
      "route": "/",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif,ico}", "/*.{css,scss,js,json}"]
  },
  "mimeTypes": {
    ".json": "text/json",
    ".css": "text/css",
    ".js": "application/javascript",
    ".html": "text/html",
    ".htm": "text/html"
  }
}